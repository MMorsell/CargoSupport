@model CargoSupport.Web.Models.PinIdModel;
@{
    ViewData["Title"] = "Home Page";
}
<link rel="stylesheet" type="text/css"
      href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.css">
<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.21/css/dataTables.bootstrap4.min.css">
<link rel="stylesheet" type="text/css"
      href="https://cdn.datatables.net/rowreorder/1.2.7/css/rowReorder.bootstrap4.min.css">

@using (Html.BeginForm("Index", "Manage", FormMethod.Post))
{
    <table cellpadding="0" cellspacing="0">
        <tr>
            <th colspan="2" align="center">Hämta från pin</th>
        </tr>
        <tr>
            <td>OrderId: </td>
            <td>
                @Html.TextBoxFor(m => m.PinId)
            </td>
        </tr>
        <tr>
            <td></td>
            <td><input type="submit" value="Submit" /></td>
        </tr>
    </table>
}

@section Scripts
{
}