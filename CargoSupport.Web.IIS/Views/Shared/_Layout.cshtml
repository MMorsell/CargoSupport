<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - EHL</title>
    <link rel="stylesheet" href="~/lib/pretty-checkbox/pretty-checkbox.css" asp-append-version="true">
    <link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap.min.css" asp-append-version="true">
    <link rel="stylesheet" href="~/lib/bootstrap/css/dataTables.bootstrap4.min.css" asp-append-version="true">
    <link rel="stylesheet" href="~/lib/bootstrap/css/rowReorder.bootstrap4.min.css" asp-append-version="true">
    <link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap-select.min.css" asp-append-version="true">
    <link rel="stylesheet" href="~/lib/flatpickr/css/flatpickr.min.css" asp-append-version="true">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/leaflet/css/leaflet.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Account" asp-action="Home">
                    <img style="height: 40px;" src="~/img/svg/ica-logo.svg" asp-append-version="true" />
                </a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                @await Html.PartialAsync("_LoginPartial")
            </div>
        </nav>
    </header>
    <div class="container-fluid">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container-fluid">
            &copy; 2020 - <a asp-area="" asp-controller="Account" asp-action="Home">MMorsell</a>
        </div>
    </footer>

    <script src="~/lib/jquery/js/jquery-3.5.1.min.js" asp-append-version="true"></script>
    <script src="~/lib/jquery/js/jquery.dataTables.min.js" asp-append-version="true"></script>
    <script src="~/lib/leaflet/js/leaflet.js" asp-append-version="true"></script>
    <script src="~/lib/popper/popper.min.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/js/bootstrap.min.js" asp-append-version="true"></script>
    <script src="~/lib/datatables/js/dataTables.bootstrap4.min.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/js/bootstrap-select.min.js" asp-append-version="true"></script>
    <script src="~/lib/flatpickr/js/flatpickr.js" asp-append-version="true"></script>
    <script src="~/lib/flatpickr/lang/sv.js" asp-append-version="true"></script>
    <script src="~/lib/moment/js/moment.min.js" asp-append-version="true"></script>
    <script src="~/lib/chart/js/Chart.min.js" asp-append-version="true"></script>
    <script src="~/lib/datatables/js/dataTables.rowReorder.min.js" asp-append-version="true"></script>
    <script src="~/lib/jspdf/js/jspdf.min.js" asp-append-version="true"></script>
    <script src="~/js/Colors.js" asp-append-version="true"></script>
    <script src="~/js/InputFunctions.js" asp-append-version="true"></script>
    <script src="~/js/ChartFunctions.js" asp-append-version="true"></script>
    <script src="~/js/ApiConstants.js" asp-append-version="true"></script>
    <script src="~/js/Constants.js" asp-append-version="true"></script>
    <script src="~/js/DatePickers.js" asp-append-version="true"></script>
    <script src="~/js/RenderFunctions.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>